{%- capture props -%}
  {
    "icon": "{{ icon }}"
    {%- if className -%},"className": "{{ className }}"{%- endif -%}
    {%- if width -%},"width": {{ width }}{%- endif -%}
    {%- if height -%},"height": {{ height }}{%- endif -%}
    {%- if stroke -%},"stroke": "{{ stroke }}"{%- endif -%}
    {%- if fill -%},"fill": "{{ fill }}"{%- endif -%}
    {%- if strokeWidth -%},"strokeWidth": {{ strokeWidth }}{%- endif -%}
    {%- if strokeLinecap -%},"strokeLinecap": "{{ strokeLinecap }}"{%- endif -%}
    {%- if strokeLinejoin -%},"strokeLinejoin": "{{ strokeLinejoin }}"{%- endif -%}
  }
{%- endcapture -%}
{%- assign props = props | fromJson -%}
{% icon props %}